{
  "name": "ttay-react-tauri-template",
  "displayName": "ttay-react-tauri-template",
  "description": "It's a test",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:ttay-react-tauri-template.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ttay-react-tauri-template.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activityBar": [
        {
          "id": "ttay-react-tauri-template",
          "title": "Test",
          "icon": "squircle.jpg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "jsonc-parser": "^3.0.0"
  }
}
